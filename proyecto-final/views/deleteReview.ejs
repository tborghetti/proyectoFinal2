<body>
    <%- include('../partials/header') %>
    <main>
    
        <h1 class="titulo">Aqui vamos a borrar las reviews</h1>
    
    <form action="/users/misResenias/delete/<%= deleteId %> " method="POST" class="resenia">
        <h1 class="borrar">Delete</h1>
        <p>Confirm user to delete review</p> 
        <div class="uk-margin">
            <div class="uk-inline">
                <span class="uk-form-icon" uk-icon="icon: user"></span>
                <input class="uk-input" name="email" type="text"  placeholder="usuario">
            </div>
        </div>
    <!-- contraseña -->
        <div class="uk-margin">
            <div class="uk-inline">
                <span class="uk-form-icon uk-form-icon-flip" uk-icon="icon: lock"></span>
                <input class="uk-input" type="password" name="password" placeholder="contraseña">
            </div>
        </div>    
        <button type="submit" name="button" class="confirmar">Confirmar</button>
      </form>
    
    </main>

    <%- include('../partials/footer') %>
</body>
</html>
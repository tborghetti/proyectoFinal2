</head>
<body>
<!-- div para la alerta de error -->
      <div class="error">
    
      </div>
      
      <header class="mainHeader">
        <div class="dosPalabras">  
        <a class="head" href= "../veoVeo/home"><label for="">Inicio</label></a>
          <a class="resenias" href= "./users/misResenias">MyReviews</a>
          </div>
        <h1>Veo<a href="../veoVeo/home"><img src="/images/logo.png" alt=""></a>Veo</h1>
          <form class="form-buscador" action="../veoVeo/resultado" method="get" id="busqueda">
            <input class="buscadorsecundario inputHidden" type="text" name="buscadorsecundario" placeholder="Buscador" value="">
            <button type="submit">Enviar</button>
            <i class="fas fa-search" id="lupita"></i>
            <a href="../veoVeo/favoritas" ><i class="fas fa-heart"></i></a>
            <a href="#offcanvas-derecho" uk-toggle><i class="fas fa-filter"></i></a>
            <a href="../users/buscar" uk-toggle><i class="fas fa-user"></i></a>
          </form>
        </header>

    <main>
      <h1> Mis rese√±as</h1>

    <% for (let i = 0; i < reseniasXusuario.length; i++) { %>

        <article class="uk-comment" >
          <header class="uk-comment-header uk-grid-medium uk-flex-middle"  uk-grid id= "miResenia">
                <!--href tieene q ir al detalle usuario--> 
                <div class="uk-comment-body" id="otrosComentarios"> 
                  <p class="puntaje"> Puntaje: <%=reseniasXusuario[i].rating%> </p>
                  <p class="comentario"><%=reseniasXusuario[i].text%></p>
                  
                  <ul class="uk-comment-meta uk-subnav uk-subnav-divider uk-margin-remove-top" >
                      <li><a href="#"><%=reseniasXusuario[i].created_at%></a></li>
                      <li><a href="misResenias/edit/<%=reseniasXusuario[i].id%>">Editar</a></li>
                      <li><a href="#">Borrar</a></li>
                  </ul>
                </div> 
          </header>
      </article>
      
   
    <% } %>
  </main>



    <%- include('../partials/footer') %>
  
</body>
</html>